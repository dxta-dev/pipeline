{
  "name": "@acme/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "npm run with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "npm lint --fix",
    "start": "npm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.4.0",
    "next": "^13.4.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@acme/eslint-config": "^0.1.0",
    "@acme/tailwind-config": "^0.1.0",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  }
}
